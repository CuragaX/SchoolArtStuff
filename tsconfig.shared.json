{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "lib": ["ESNext", "WebWorker", "DOM", "DOM.Iterable"],
    "baseUrl": "./src",
    "composite": true,
    "paths": {
      "$aero_config": ["./src/config.ts"],
      "$aero_shared": ["shared/*"]
    }
  },
  "include": [
    "src/shared/**/*",
    "./config.ts",
    "src/AeroSandbox/src/sandboxers/JS"
  ]
}
