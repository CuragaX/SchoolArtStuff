{
  "name": "aero",
  "version": "1.0.0",
  "description": "THE interception proxy",
  "main": "src/this/handle.ts",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "tasks": [
    {
      "type": "npm",
      "script": "build",
      "group": "build",
      "problemMatcher": [
        "$ts-webpack",
        "$tslint-webpack"
      ]
    },
    {
      "type": "npm",
      "script": "buildEslint",
      "group": "build",
      "problemMatcher": [
        "$ts-webpack",
        "$ts-checker-eslint-webpack-watch"
      ]
    },
    {
      "type": "npm",
      "script": "buildRs",
      "group": "build",
      "problemMatcher": [
        "$ts-webpack",
        "$ts-checker-eslint-webpack-watch"
      ]
    }
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "npx prettier -w .",
    "build": "webpack",
    "buildDebug": "webpack --stats-error-detail --watch --info-verbosity",
    "buildRs": "rspack build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProxyHaven/aero.git"
  },
  "keywords": [
    "Proxy",
    "Archiver",
    "Unblock",
    "Interception"
  ],
  "author": "Ryan Wilson",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ProxyHaven/aero/issues"
  },
  "homepage": "https://github.com/ProxyHaven/aero#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.237",
    "@types/webpack-env": "^1.18.1",
    "bun-types": "^1.0.13",
    "esbuild": "0.19.7",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "@biomejs/biome": "^1.3.3"
  },
  "dependencies": {
    "@mercuryworkshop/bare-as-module3": "^2.2.2",
    "@mercuryworkshop/bare-mux": "^2.0.2",
    "cld3-asm": "^3.1.1",
    "esbuild-loader": "^4.0.1",
    "esbuild-wasm": "^0.23.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1"
  }
}
