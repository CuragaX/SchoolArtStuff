{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "lib": ["ESNext", "WebWorker", "WebWorker.ImportScripts"],
    "baseUrl": "src",
    "composite": true,
    "paths": {
      "$aero_config": ["./src/config.ts"],
      "$aero_this": ["this/*"],
      "$aero_shared": ["shared/*"]
    }
  },
  "include": ["config.ts", "src/this/**/*.ts", "index.d.ts", "index.d.ts"]
}
