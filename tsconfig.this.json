{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext", "WebWorker", "WebWorker.ImportScripts"],
		"composite": true,
		"moduleResolution": "nodenext",
		"baseUrl": "src",
		"paths": {
			"this/*": ["this/*"],
			"shared/*": ["shared/*"],
			"config": ["./config.ts"],
			"mwTypes": ["middleware/index.d.ts"]
		},
	},
	"include": ["src/this/**/*.ts", "types/*.d.ts"]
}
