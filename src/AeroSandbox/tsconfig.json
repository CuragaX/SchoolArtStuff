{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "baseUrl": "./",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "paths": {
      "$aero/*": ["*"],
      "$aero_sw/*": ["../../*"],
      "$src/*": ["src/*"],
      "$sandbox/*": ["src/sandboxers/*"],
      "$apis/*": ["src/apis/*"],
      "$cors/*": ["src/cors/*"],
      "$shared/*": ["src/shared/*"],
      "$types/*": ["types/*"]
    },
    "types": ["./types/index.d.ts", "@types/trusted", "@types/websql"]
  },
  "include": [
    "src/**/*.ts",
    "build/customBuilds/**/*.ts",
    "./config.ts",
    "types/**/*.d.ts",
    "src/interceptors/concealer/element.ts.old",
    "config.ts"
  ]
}
